<?xml version="1.0"?>
<launch>
  <!-- Create an instance of each appropriate node -->
  <node name="youbot_joy_arm" pkg="youbot_joy_arm" type="youbot_joy_arm" respawn="false" output="screen" />
  <node name="joy_node" pkg="joy" type="joy_node" respawn="false" output="screen">
    <param name="autorepeat_rate" value="10"/>
  </node>
  
</launch>
